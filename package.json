{
  "name": "eslint-plugin-opinionated-vue-ts",
  "version": "1.0.0",
  "description": "Opinionated ESLint rules and flat config for Vue + TypeScript projects.",
  "license": "MIT",
  "keywords": [
    "eslint",
    "eslint-plugin",
    "eslint-config",
    "vue",
    "typescript",
    "vitest",
    "flat-config"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ddavid93/opinionated-eslint-vue-ts.git"
  },
  "bugs": {
    "url": "https://github.com/ddavid93/opinionated-eslint-vue-ts/issues"
  },
  "homepage": "https://github.com/ddavid93/opinionated-eslint-vue-ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./configs/full": {
      "import": "./dist/configs/full.mjs",
      "types": "./dist/configs/full.d.ts"
    }
  },
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "tsdown",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm build",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "prepare": "husky"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@eslint/js": "9.39.2",
    "@vitest/eslint-plugin": "1.6.7",
    "@vue/eslint-config-prettier": "10.2.0",
    "@vue/eslint-config-typescript": "14.6.0",
    "eslint": "9.39.2",
    "eslint-plugin-import-x": "4.16.1",
    "eslint-plugin-oxlint": "1.46.0",
    "eslint-plugin-pnpm": "1.5.0",
    "eslint-plugin-prettier": "5.5.5",
    "eslint-plugin-unicorn": "63.0.0",
    "eslint-plugin-vue": "10.7.0",
    "globals": "17.3.0",
    "typescript-eslint": "8.55.0"
  },
  "devDependencies": {
    "conventional-changelog-cli": "^5.0.0",
    "husky": "9.1.7",
    "tsdown": "^0.20.3"
  }
}
